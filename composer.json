{
    "name": "cultuurnet/symfony-security-jwt",
    "description": "JWT implementation for the Symfony Security component",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "symfony/security": "~2.7.3",
        "cultuurnet/udb3-jwt": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\SymfonySecurityJwt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\SymfonySecurityJwt\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
